@model IEnumerable<ALPHA_DGS.Models.MagazijnPartij>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pvan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ptot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PHerk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pstadid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VpNaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MlokId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uitserie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AantFust)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Naam)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pvan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ptot)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PHerk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pstadid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VpNaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MlokId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uitserie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AantFust)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Naam)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
